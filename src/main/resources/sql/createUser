CREATE DATABASE `test_user` /*!40100 COLLATE 'utf8_general_ci' */
CREATE TABLE `test_user` (
	`id` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT COMMENT '主键',
	`username` VARCHAR(20) NULL DEFAULT NULL COMMENT '姓名' COLLATE 'utf8_general_ci',
	`age` VARCHAR(20) NULL DEFAULT NULL COMMENT '年龄' COLLATE 'utf8_general_ci'
	PRIMARY KEY (`id`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=2
AVG_ROW_LENGTH=1
MAX_ROWS=100000
;

